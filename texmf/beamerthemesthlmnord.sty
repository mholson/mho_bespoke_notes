%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
\setbeamertemplate{footline}{%
  \hbox{%
    \begin{beamercolorbox}[
        wd=.9\paperwidth,
        ht=2ex,
        dp=1ex,
        center]{section in head/foot}%
      {\usebeamerfont{section in head/foot}\texttt{\currfilename}}
    \end{beamercolorbox}%
    \begin{beamercolorbox}[
      wd=.1\paperwidth,
      ht=2ex,
      dp=1ex,
      center]{page number in head/foot}
      {\usebeamerfont{page number in head/foot} \insertframenumber\ {\usebeamercolor[fg]{footnote book}\cfLightTeal\faBookOpen}\ \inserttotalframenumber}
    \end{beamercolorbox}%
  }
}
