\if@dosectionpages

  \AtBeginSection[]{
    \begin{frame}[plain,noframenumbering]
      \vfill
      \begin{center}
        {\usebeamerfont{section page}\usebeamercolor*[fg]{section page}\insertsectionhead}
      \end{center}
      \par%
      \vfill%
    \end{frame}
  }
  \AtBeginSubsection[]{
    \begin{frame}[plain,noframenumbering]
      \vfill
      \begin{center}
        {\usebeamerfont{section page}\usebeamercolor*[fg]{section page}\insertsectionhead}
        \par%
        {\usebeamerfont{subsection page}\usebeamercolor*[fg]{subsection page}\insertsubsectionhead}
      \end{center}
      \par%
      \vfill%
    \end{frame}
  }
\else
\fi
