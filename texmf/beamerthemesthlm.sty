%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%   Frame Header
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
\setbeamertemplate{frametitle}
{
    \nointerlineskip
    \begin{beamercolorbox}[wd=\paperwidth,leftskip=0.2cm,ht=0.6cm,dp=0.25cm]{header primary}%
        \usebeamerfont{header title}\insertframetitle \
        {\usebeamercolor*[fg]{header title}\usebeamerfont{header subtitle}\insertframesubtitle}
        \hfill
    \end{beamercolorbox}%
    \nointerlineskip
}
%  END =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%  Frame Footer
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
% > > >   Define the footline
\setbeamertemplate{footline}{%
  \hbox{%
    \begin{beamercolorbox}[
      wd=.9\paperwidth,
      ht=2ex,
      dp=1.75ex,
      center]{section in head/foot}%
      % > > > display priority: subsection > section > title
      \texttt{\filename}
    \end{beamercolorbox}%
    \begin{beamercolorbox}[
      wd=.1\paperwidth,
      ht=2ex,
      dp=1.75ex,
      center]{number in head/foot}
      \insertframenumber\ /\ \inserttotalframenumber
    \end{beamercolorbox}%
  }
}
%  END =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%   Footnotes
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
\hfuzz=\maxdimen % Remove warning hbox overfull (TODO) 
\renewcommand\footnoterule{\color{sthlmGrey!50}\hspace*{-0.5cm}\rule{0.95\paperwidth}{0.2pt}\vspace*{0.07cm}}
\setbeamertemplate{footnote}{
	\begin{beamercolorbox}[wd=\textwidth, leftskip=0.3cm,rightskip=0.3cm]{footer text}%
	\usebeamerfont{footnote}\insertfootnotemark\insertfootnotetext
	\end{beamercolorbox}%
}
%  END =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%   Deck Title Page
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
\def\titlepage{\usebeamertemplate{title page}}
\def\logo@width{0.1\paperwidth}
\def\logo@height{0.1\paperheight}
\tikzset{
  sider/.style={
    draw=none,
    rectangle,
    outer xsep=-0.1mm,
    minimum height=\paperheight,
    minimum width=\logo@width,
    fill=sthlmPrimaryGreen
  },
  icon/.style={
  	draw=sthlmPrimaryGreen,
		line width=1.2pt,
  	fill=sthlmPrimaryLightGreen, 
  	minimum height=\logo@height,
  	minimum width=\logo@width,
  	text=sthlmBGGrey,
  	outer xsep=0.4cm,
  	outer ysep=0.4cm,
  	inner sep=0.1cm,
  }
}
\setbeamertemplate{title page}
{
	\begin{frame}[plain]
    	\begin{tikzpicture}[remember picture,overlay]
    		\hspace*{-1pt}\node[anchor=west,sider] at (current page.west){};
    		%\node[anchor=south west,sider, fill=primary, minimum height=0.95\paperheight] at (current page.south west){};
			\node[align=left] at (current page.center){
				\usebeamerfont{title}\color{sthlmPrimaryLightGreen}\inserttitle\\[0.3cm]
				\usebeamerfont{subtitle}\color{sthlmPrimaryGreen}\insertsubtitle\\[0.1cm]
				\usebeamercolor[fg]{footer}\rule{0.7\paperwidth}{0.2pt}	\\[0.35cm]
				\usebeamerfont{author}Created by: \color{sthlmPrimaryLightGreen}\insertauthor\\[0.3cm]
				\usebeamerfont{institute}Institute: \color{sthlmPrimaryLightGreen}\insertinstitute\\[0.3cm]
				\usebeamerfont{institute}Course: \color{sthlmPrimaryLightGreen}\course \\[0.3cm]
				\usebeamerfont{institute}File: \color{sthlmPrimaryLightGreen}\filename
				%\usebeamerfont{author}\small{FileID: \color{sthlmPrimaryLightGreen}\fileid} 
			 %\includegraphics[width=\logo@width, height=\logo@height, keepaspectratio]{assets/titlelogo}
			};
			\node[anchor=south,outer ysep=0.4cm] at (current page.south){\small\color{sthlmGrey!90} \LaTeX ed on \insertdate};
			%\node[anchor=south east,icon] at (current page.south east)(logo){\includegraphics[width=\logo@width, height=\logo@height, keepaspectratio]{\inserttitlegraphic}};
			\node[anchor=south east] at (current page.south east)(logo){\includegraphics[width=\logo@width, height=\logo@height, keepaspectratio]{\inserttitlegraphic}};
		\end{tikzpicture}
	\end{frame}
}
%  END =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%   Section Frame
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
\if@dosectionpages

\AtBeginSection[]{
  \begin{frame}[plain,noframenumbering]
    \vfill
    \center
    \usebeamerfont{section page}
    \usebeamercolor[fg]{section page}
    \insertsectionhead
    \par
    \vfill
  \end{frame}
}
\AtBeginSubsection[]{
  \begin{frame}[plain,noframenumbering]
    \vfill
    \center
    \usebeamerfont{section page}
    \usebeamercolor[fg]{section page}
    \insertsectionhead
    \par%
    \usebeamerfont{subsection page}
    \usebeamercolor[fg]{subsection page}
    \insertsubsectionhead
    \par%
    \vfill
  \end{frame}
}
\else
\fi
%  END =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

%=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
%   Table of Contents
