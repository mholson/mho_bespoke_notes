\setbeamercolor*{header primary}{fg=white,bg=sthlmPrimaryGreen}
\setbeamercolor*{header secondary}{fg=white,bg=sthlmPrimaryLightGreen}
\setbeamercolor*{secondary}{fg=sthlmOrange!10,bg=sthlmOrange}
\setbeamercolor*{example text}{fg=sthlmPrimaryGreen}
\setbeamercolor*{normal text}{fg=sthlmBlack}
\setbeamercolor*{alerted text}{fg=sthlmOrange}
\setbeamercolor*{itemize item}{fg=sthlmPrimaryGreen}
\setbeamercolor*{itemize subitem}{fg=sthlmPrimaryGreen}
